<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EQ.Domain.v2" namespace="EQ.Domain.v2">
  <class name="Address" table="EQ_ADDRESS" xmlns="urn:nhibernate-mapping-2.2" dynamic-update="false" select-before-update="true">
    <id name="Id" column="Id" type="Int64" unsaved-value="0">
      <generator class="sequence">
        <param name="sequence">DEPARTMENT_SEQ</param>
      </generator>
    </id>
    <property name="Region" column="REGION" length="100"/>
    <property name="District" column="DISTRICT" length="100"/>
    <property name="City" column="CITY" length="100"/>
    <property name="Street" column="ADDRESS" length="150"/>
  </class>
</hibernate-mapping>